{
    "configurations": [
        {
            "name": "cuda/hip",
            "includePath": [
                "${workspaceFolder}/**",
                "~/Code/pyenvs/cudaenv/lib/python3.12/site-packages/torch/include/**",
                "~/miniconda3/include/python3.12/**",
                "/opt/rocm-6.1.3/include/**",
            ],
            "defines": [
                "__HIP_PLATFORM_AMD__",
                "__AMD__"
